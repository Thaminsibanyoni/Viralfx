{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "noEmit": false
  },
  "include": ["src"],
  "exclude": [
    "src/**/__tests__",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "node_modules",
    "dist"
  ]
}