{
  "name": "viralfx-backend",
  "version": "1.0.0",
  "description": "ViralFX Backend API - Real-time social momentum trading platform",
  "author": "ViralFX Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "node scripts/validate-build.js && nest build --builder swc",
    "build:fast": "node scripts/validate-build.js && NODE_OPTIONS=\"--max-old-space-size=8192\" nest build --webpack",
    "build:swc": "node scripts/validate-build.js && NODE_OPTIONS=\"--max-old-space-size=8192\" nest build --builder swc",
    "build:prod": "node scripts/validate-build.js && NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=8192\" nest build --webpack",
    "build:dev": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "prestart:dev": "node scripts/check-prisma-changes.js && npm run prisma:generate && node scripts/update-prisma-cache.js || echo \"Prisma schema unchanged, skipping generation\"",
    "start:dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" nest start --watch",
    "start:fast": "NODE_OPTIONS=\"--max-old-space-size=8192\" nest start --watch --webpack",
    "start:swc": "NODE_OPTIONS=\"--max-old-space-size=8192\" nest start --watch --builder swc",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:check": "node scripts/check-prisma-changes.js",
    "prisma:force": "npm run prisma:generate",
    "dev:profile": "NODE_ENV=development node --prof scripts/profile-startup.js",
    "cache:clear": "rm -rf dist .nest node_modules/.cache .prisma-cache.json",
    "cache:clear:all": "npm run cache:clear && rm -rf node_modules/.cache && npm install",
    "circular:check": "madge --circular --extensions ts src/",
    "circular:check:json": "madge --circular --extensions ts --json src/ > circular-deps.json",
    "circular:graph": "madge --circular --extensions ts --image circular-deps.png src/",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:verify": "ts-node scripts/verify-prisma-sync.ts",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "seed:superadmin": "ts-node scripts/seed-superadmin-prisma.ts",
    "api:seed-products": "ts-node scripts/seed-api-products.ts",
    "api:generate-invoice": "ts-node scripts/generate-api-invoice.ts",
    "api:reset-quotas": "ts-node scripts/reset-api-quotas.ts",
    "start:validator": "nest start validator-node",
    "oracle:dev": "docker-compose -f docker-compose.oracle.yml up",
    "oracle:build": "docker-compose -f docker-compose.oracle.yml build",
    "oracle:down": "docker-compose -f docker-compose.oracle.yml down",
    "worker:orders": "nest start --watch --entryFile worker-orders",
    "worker:wallets": "nest start --watch --entryFile worker-wallets",
    "worker:all": "concurrently \"npm run worker:orders\" \"npm run worker:wallets\"",
    "verify": "npm run prisma:verify && npm run build:fast",
    "build:analyze": "NODE_ENV=production ANALYZE=true npm run build:prod",
    "postinstall": "prisma generate || echo 'Prisma generation skipped'",
    "prebuild": "node scripts/validate-build.js"
  },
  "dependencies": {
    "@googleapis/youtube": "^28.0.0",
    "@jridgewell/sourcemap-codec": "^1.5.5",
    "@nestjs-modules/ioredis": "^2.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/websockets": "^10.2.7",
    "@prisma/client": "^5.5.2",
    "@sendgrid/mail": "^8.1.6",
    "@socket.io/redis-adapter": "^7.2.0",
    "@tensorflow/tfjs": "^4.22.0",
    "aws-sdk": "^2.1441.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "bullmq": "^3.16.2",
    "cache-manager": "^5.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "express": "^5.2.1",
    "facebook-nodejs-business-sdk": "^19.0.0",
    "firebase-admin": "^13.6.0",
    "googleapis": "^169.0.0",
    "helmet": "^7.0.0",
    "instagram-graph-api": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.2",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nestjs-minio-client": "^2.2.0",
    "nodemailer": "^7.0.11",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "pusher": "^5.2.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "tesseract.js": "^5.0.0",
    "tiktok-business-api-sdk-official": "^1.0.0",
    "twilio": "^5.10.7",
    "twitter-api-v2": "^1.27.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-loki": "^6.0.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.3.96",
    "@types/bcrypt": "^5.0.1",
    "@types/compression": "^1.7.4",
    "@types/crypto-js": "^4.1.4",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/multer": "^1.4.8",
    "@types/multer-s3": "^3.0.2",
    "@types/node": "^20.8.3",
    "@types/passport-apple": "^2.0.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.12",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.2",
    "@types/speakeasy": "^2.0.9",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^13.0.0",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.14",
    "thread-loader": "^4.0.2",
    "tmp": "^0.2.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
