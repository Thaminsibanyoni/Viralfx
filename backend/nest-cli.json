{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "typeCheck": false,
    "generateOptions": {
      "spec": false
    },
    "assets": ["**/*.prisma"],
    "watchAssets": true,
    "builder": "swc",
    "preserveWatchOutput": true,
    "failSilently": false
  },
  "projects": {}
}